<%= form_for @category do |f| %>
  <div class="field">
    <%= f.label :title, "タイトル" %>
    <%= f.text_field :title, class: "form-control" %>
  </div>

  <% @questions.each do |question| %>
    <%= question.title %>
  <% end %>

  <div class="action" style="margin-top: 1em">
    <%= f.submit "登録", class: "btn btn-primary", style: "margin-bottom: 1em" %>
  </div>
<% end %>
